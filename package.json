{
    "name": "redmudimport",
    "version": "0.0.2",
    "description": "Import tool for RedMUD.",
    "main": "import.js",
    "scripts": {
        "test": "mocha --trace-warnings --reporter spec",
        "testc": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha",
        "jsdoc": "./node_modules/.bin/jsdoc ./export.js ./lib -r --readme ./README.md -d docs",
        "wiki": "./node_modules/.bin/jsdoc2md export.js lib/*/*.js > api.md",
        "start": "node import"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jyurkiw/RedMUDImport.git"
    },
    "keywords": [
        "RedMUD",
        "mud",
        "game"
    ],
    "author": "Jeffrey Yurkiw",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jyurkiw/RedMUDImport/issues"
    },
    "homepage": "https://github.com/jyurkiw/RedMUDImport#readme",
    "dependencies": {
        "fs": "0.0.1-security",
        "redmudlib": "git+https://github.com/jyurkiw/RedMUDLib.git"
    },
    "devDependencies": {
        "chai": "^3.5.0",
        "istanbul": "^0.4.5",
        "jsdoc": "^3.4.2",
        "jsdoc-to-markdown": "^2.0.1",
        "jshint": "^2.9.4",
        "mocha": "^3.1.2"
    }
}